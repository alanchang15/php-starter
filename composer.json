{
    "name": "alanchang15/php-starter",
    "authors": [
        {
            "name": "Alan Chang",
            "email": "alanchang15@gmail.com",
            "role": "Developer"
        }
    ],    
    "require": {
        "php": ">=5.3.0",
        "corneltek/pux": "~1.3",
        "mikecao/flight": "1.2",
        "illuminate/support": "4.1",
        "illuminate/database": "*",
        "twig/twig": "1.*",
        "twig/extensions": "*"
    },
    "autoload": {
        "classmap": [
            "app/modules/" 
        ],
        "psr-4": {
            "Modules\\": "app/modules/",
            "" : [
                "app/src/",
                "app/controllers/",
                "app/models/"
            ]
        }
    }
}
